    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "lint": "eslint .",
    "clean": "rm -rf dist",
    "tsd": "tsd --files src/index.test-d.ts",
    "quality": "pnpm type-check && pnpm test && pnpm build && pnpm tsd && pnpm lint"
