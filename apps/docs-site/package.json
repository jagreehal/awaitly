{
  "name": "awaitly-docs",
  "type": "module",
  "version": "1.2.0",
  "private": true,
  "description": "Documentation for awaitly",
  "scripts": {
    "dev": "pnpm copy-wasm && BASE=/ astro dev",
    "start": "pnpm copy-wasm && BASE=/ astro dev",
